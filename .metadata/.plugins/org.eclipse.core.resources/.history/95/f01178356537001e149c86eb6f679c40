package assignment2;

import java.util.Scanner;

public class JumpingStatements
{

public static void main(String args[]) {
	Scanner sc=new Scanner(System.in);
	System.out.println("Enter a number");
	int num =sc.nextInt();
	
	for(int i=0;i<5;i++)
	{
		System.out.println(i);
		if(i==num)
		{
		break;	
		}
	}
	
	for(int i=0;i<10;i++)
	{
		System.out.println(i);
		if(i==9)
		{
		continue;	
		}
	}
	
	
}

}
